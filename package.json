{
  "name": "dardcor-ai",
  "version": "1.0.0",
  "description": "Dardcor AI",
  "main": "server.js",
  "engines": {
    "node": "24.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "watch:css": "tailwindcss -i ./input.css -o ./public/css/tailwind.css --watch",
